#ifndef TESTSUITE_H
#define TESTSUITE_H
#include <stdio.h>
#include <spu_intrinsics.h>
#include <primitive.h>

void PRINT_VEC_FLOAT4(vec_float4 t) {
  mix_float4 v = (mix_float4)t;
  int i = 0;
  for(; i < 4; i ++) {
    printf(" %.3f", v.a[i]);
  }
  puts("");
}

void PRINT_VEC_UINT4(vec_uint4 t) {
  mix_uint4 v = (mix_uint4)t;
  int i = 0;
  for(; i < 4; i ++) {
    printf(" %d", v.a[i]);
  }
  puts("");
}

#endif
